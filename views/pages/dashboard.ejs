  <!-- The Band Section -->
<div class="w3-container w3-content w3-center w3-padding-64" style="max-width:100%" id="twitter">
	<h2 class="w3-wide">TWITTER</h2>
	<div class="twitter">
		<div class="twitter-elem">
			<a class="twitter-timeline" data-width="500" data-height="600" data-theme="light" href="https://twitter.com/TwitterDev">
			</a>
			<script async src="https://platform.twitter.com/widgets.js" charset="utf-8">
			</script>
		</div>
		<div class="twitter-elem">
			<blockquote class="twitter-tweet" data-lang="fr">
				<p lang="en" dir="ltr">
					<a href=<%= twitter_quote %>>@GrandTetonNPS</a>. 
					<a href=<%= twitter_quote %>>#nature</a> 
					<a href=<%= twitter_quote %>>#sunset</a> 
					<a href="http://t.co/YuKy2rcjyU">pic.twitter.com/YuKy2rcjyU</a>
				</p>
				&mdash; US Department of the Interior (@Interior) 
				<a href=<%= twitter_quote %>>>5 mai 2014</a>
			</blockquote>
			<script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
		</div>
	</div>
</div>

<div class="w3-black" id="facebook">
  <!-- The Tour Section -->
